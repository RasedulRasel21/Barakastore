<style>
 .custom-ins-class{
   margin: 0 auto;
   padding: 0;
   padding: 0;
   border-radius: 20px;
   max-width: 1400px;
 }
.cus-insta-text-part{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    max-width: 1000px;
    margin: 0 auto;
    border-radius: 20px 20px 0 0;
}

.cus-insta-text-part h1 {
    font-size: 36px;
    font-weight: 600;
    line-height: 44px;
    color: #000;
    padding-top: 40px;
    margin: 0;
    }
  .cus-insta-text-part a {
    margin: 0;
    text-decoration: none;
    color: #000;
  }

.custom-section-for-instagram-mb{
    display: none;
}

.custom-section-for-instagram {
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 30px 5rem 0;
    gap: 10px;
    border-radius: 0 0 20px 20px;
    margin-bottom: 60px;
}


.cus-sec-left-part {
    display: grid;
    grid-template-columns: 1fr 1fr 1.5fr;
    grid-template-rows: repeat(2, 1fr);
    gap: 10px;
    max-width: 50%;
    height: 600px;
}


.cus-image {
    position: relative;
    overflow: hidden;
    border-radius: 8px;
}

.cus-image img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: transform 0.3s ease;
}

.cus-image video {
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: transform 0.3s ease;
}


.cus-image img:hover {
    transform: scale(1.05);
}


.image-1 {
    grid-column: 1 / span 2;
    grid-row: 1;
}

.image-2 {
    grid-column: 3;
    grid-row: 1 / span 2;
}

.image-3 {
    grid-column: 1;
    grid-row: 2;
    position: relative;
}

.video-icon {
    position: absolute;
    top: 10px;
    right: 10px;
}

img.video-icon {
    width: unset;
    height: unset;
}

.image-4 {
    grid-column: 2;
    grid-row: 2;
}


.cus-sec-right-part {
    display: grid;
    grid-template-columns: 1.5fr 1fr 1fr;
    grid-template-rows: repeat(2, 1fr);
    max-width: 50%;
    height: 600px;
}


.image-5 {
    grid-column: 1;
    grid-row: 1 / span 2;
    position: relative;
    margin-right: 5px;
}


.image-6 {
    grid-column: 2;
    grid-row: 1;
    margin-right: 5px;
    margin-left: 5px;
    margin-bottom: 5px;
}

.image-7 {
    grid-column: 3;
    grid-row: 1;
    margin-left: 5px;
    margin-bottom: 5px;
}

.image-8 {
    grid-column: 2 / span 3;
    grid-row: 2;
    margin-top: 5px;
    margin-left: 5px;
}

.lightbox {
    display: none;
    position: fixed;
    z-index: 1000;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.8);
    justify-content: center;
    align-items: center;
}

.lightbox-content {
    position: relative;
    max-width: 90%;
    max-height: 90%;
    display: flex;
    justify-content: center;
    align-items: center;
}

.lightbox-content img, .lightbox-content video {
     max-width: 432px;
     min-height: 90vh;
     object-fit: cover; 
     border-radius: 15px;
     min-width: 432px;
}
  .lightbox.active .lightbox-content img, 
.lightbox.active .lightbox-content video {
    display: block;
}


.close {
    position: absolute;
    top: 20px;
    right: 30px;
    font-size: 40px;
    color: white;
    cursor: pointer;
    z-index: 1;

}
.prev, .next {
    position: absolute;
    top: 50%;
    font-size: 20px;
    font-weight: bold;
    color: #000000;
    cursor: pointer;
    background: #d3dbe2;
    padding: 5px 15px;
    border-radius: 50%;
    user-select: none;
    height: 50px;
    width: 50px;
    display: flex
;
    align-items: center;
    justify-content: center;
}

.next:hover {
    background: #ffffff;
    transition: 0.3s;
  }
  .prev:hover {
    background: #ffffff;
    transition: 0.3s;
  }

.prev {
    left: 36%;
    z-index: 1;

}

.next {
    right: 36%;
    z-index: 1;

}
  
@media (min-width: 768px) {
  .next {
    right: 50%;
    left: 50%;
    z-index: 1;
    transform: translate(236px, -50%);
}
    .prev {
    right: 50%;
    left: 50%;
    z-index: 1;
    transform: translate(-283px, -50%);
}
}


@media (max-width: 767px) {
  .custom-section-for-instagram{
    display: none;
  }
 .custom-ins-class{
    margin: 40px 15px;
 }
  .cus-insta-text-part h1 {
    font-size: 20px;
    max-width: 100%;
    padding-top: 0;
  }

  .custom-section-for-instagram-mb {
    display: flex;
    justify-content: center;
    align-items: center;
    max-width: 100%;
    padding: 20px 0 0 0;
    border-radius: 0 0 20px 20px;
}


.cus-sec-mb-part {
    display: grid;
    grid-template-columns: 1fr 1fr;
    grid-template-rows: repeat(6, 1fr);
    max-width: 100%;
    height: 550px;
    width: -webkit-fill-available;
}


.cus-image {
    position: relative;
    overflow: hidden;
    border-radius: 8px;
}

.cus-image img {
    width: 100%;
    height: 100%;
    object-fit: fill;
    transition: transform 0.3s ease;
}

.cus-image video {
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: transform 0.3s ease;
}


.cus-image img:hover {
    transform: scale(1.05);
}


.image-1 {
    grid-column: 1;
    grid-row: 1;
    margin-bottom: 5px;
    margin-right: 5px;
}

.image-2 {
    grid-column: 1 ;
    grid-row: 2 / span 3;
    position: relative;
    margin-bottom: 5px;
    margin-right: 5px;
    margin-top: 5px;
    
}

.image-3 {
    grid-column: 1 ;
    grid-row: 5 / span 6;
    margin-right: 5px;
    margin-top: 5px;
}

.image-4 {
    grid-column: 2;
    grid-row: 1 / span 2;
    margin-bottom: 5px;
    margin-left: 5px;
  }

.image-5 {
    grid-column: 2;
    grid-row: 3 / span 6;
    position: relative;
    margin-left: 5px;
    margin-top: 5px;
}
  .prev {
    left: 20px !important;
    z-index: 1;

}

.next {
    right: 20px !important;
    z-index: 1;

}

  .lightbox-content img, .lightbox-content video {
     max-width: 100%;
     min-height: 100vh;
     object-fit: cover; 
     border-radius: 15px;
}



}


