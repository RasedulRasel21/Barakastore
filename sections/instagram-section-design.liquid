<div id="lightbox" class="lightbox">
    <span class="close">&times;</span>
    <span class="prev">&#10094;</span> <!-- Left arrow -->
    <span class="next">&#10095;</span> <!-- Right arrow -->
    <div class="lightbox-content">
        <img id="lightbox-img" src="" alt="">
        <video id="lightbox-video" controls>
            <source id="lightbox-video-source" src="" type="video/mp4">
            Your browser does not support the video tag.
        </video>
    </div>
</div>



      </div>

 <script>
document.addEventListener("DOMContentLoaded", function () {
    const lightbox = document.getElementById("lightbox");
    const lightboxImg = document.getElementById("lightbox-img");
    const lightboxVideo = document.getElementById("lightbox-video");
    const lightboxVideoSource = document.getElementById("lightbox-video-source");
    const closeBtn = document.querySelector(".close");
    const prevBtn = document.querySelector(".prev");
    const nextBtn = document.querySelector(".next");
    const mediaItems = Array.from(document.querySelectorAll(".cus-image img, .cus-image video"));
    let currentIndex = 0;

    function showMedia(index) {
        if (index < 0) index = mediaItems.length - 1; // Loop to last item
        if (index >= mediaItems.length) index = 0; // Loop to first item

        currentIndex = index;
        const item = mediaItems[currentIndex];

        // Hide all elements with class .icon-hide when opening the lightbox
        document.querySelectorAll('.icon-hide').forEach(el => el.style.display = 'none');

        lightbox.style.display = "flex";
        if (item.tagName === "IMG") {
            lightboxImg.src = item.src;
            lightboxImg.style.display = "block";
            lightboxVideo.style.display = "none";
        } else if (item.tagName === "VIDEO") {
            lightboxVideoSource.src = item.querySelector("source").src;
            lightboxVideo.load();
            lightboxVideo.style.display = "block";
            lightboxImg.style.display = "none";
            lightboxVideo.play();
            lightboxVideo.muted = true; // Corrected this line
        }
    }

    // Open lightbox on click
    mediaItems.forEach((item, index) => {
        item.addEventListener("click", function () {
            showMedia(index);
        });
    });

    // Close lightbox
    closeBtn.addEventListener("click", function () {
        lightbox.style.display = "none";
        lightboxVideo.pause();

        // Show all elements with class .icon-hide again when closing the lightbox
        document.querySelectorAll('.icon-hide').forEach(el => el.style.display = 'block');
    });

    // Next button click
    nextBtn.addEventListener("click", function () {
        showMedia(currentIndex + 1);
    });

    // Prev button click
    prevBtn.addEventListener("click", function () {
        showMedia(currentIndex - 1);
    });

    // Close lightbox when clicking outside the content
    lightbox.addEventListener("click", function (e) {
        if (e.target === lightbox) {
            lightbox.style.display = "none";
            lightboxVideo.pause();

            // Show all elements with class .icon-hide again when clicking outside
            document.querySelectorAll('.icon-hide').forEach(el => el.style.display = 'block');
        }
    });

    // Keyboard navigation
    document.addEventListener("keydown", function (e) {
        if (lightbox.style.display === "flex") {
            if (e.key === "ArrowRight") showMedia(currentIndex + 1); // Next
            if (e.key === "ArrowLeft") showMedia(currentIndex - 1); // Previous
            if (e.key === "Escape") {
                lightbox.style.display = "none";
                lightboxVideo.pause();

                // Show all elements with class .icon-hide again when escaping
                document.querySelectorAll('.icon-hide').forEach(el => el.style.display = 'block');
            }
        }
    });
});



</script>



